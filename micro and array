#include<iostream>
#include<algorithm>
using namespace std;
int main(){
	int t;
	cin>>t;
	while(t--){
		int n,k;
		cin>>n>>k;
		int arr[n];
		for (int i=0;i<n;i++){
			cin>>arr[i];
		}
		sort(arr,arr+n);
		int check=k-arr[0];
		if(check<0){
			cout<<"0"<<endl;
		}
		else {
		cout<<check<<endl;}
		
	}
}
